<h1>{{  section.settings.text }}</h1>
{% for block in section.blocks %}
  <a href="{{ block.settings.url }}">{{ block.settings.text }}</a>
  {% endfor %}


{% schema%}
{
  "name": "Custom Section",
  "settings": [
    {
    "type": "text",
    "id": "text",
"label": "text" }
  ],
  "presets": [
    {"name": "default custom section"}],
    "tag": "header",
    "class": "custom-section", 
    "limit" : 1, 
    "blocks" : [{
    "name": "custom-block",
    "type": "social",
    "settings": [
   {
    "type": "text",
    "id": "text",
"label": "text" },
   {
    "type": "url",
    "id": "url",
"label": "url" }
    ]

  }]
    
  
}
{% endschema %}